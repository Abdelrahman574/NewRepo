		{
			"name": "Top stations by clicks",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response is JSON\", () => {\r",
							"    pm.response.to.be.json;\r",
							"});\r",
							"\r",
							"pm.test(\"Response is array\", () => {\r",
							"    const body = pm.response.json();\r",
							"    pm.expect(Array.isArray(body)).to.be.true;\r",
							"});\r",
							"\r",
							"pm.test(\"Stations have 'name' and 'url_resolved'\", () => {\r",
							"    const body = pm.response.json();\r",
							"    body.forEach(st => {\r",
							"        pm.expect(st).to.have.property(\"name\");\r",
							"        pm.expect(st).to.have.property(\"url_resolved\");\r",
							"    });\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
